{
  "name": "nestjs-api-tutorial",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prisma:dev:deploy": "prisma migrate deploy",
    "db:dev:rm": "docker-compose rm dev-db",
    "db:dev:up": "docker-compose up dev-db",
    "db:dev:restart": "npm run db:dev:rm && npm run db:dev:up && npm run prisma:dev:deploy",
    "prisma:test:deploy": "dotenv -e .env.test -- prisma migrate deploy",
    "db:test:rm": "docker-compose rm test-db",
    "db:test:up": "docker-compose up test-db",
    "db:test:restart": "npm run db:test:rm && npm run db:test:up && sleep 1 && npm run prisma:test:deploy",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:server": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:e2e": "npm run db:test:restart",
    "test:e2e": "dotenv -e .env.test -- jest --watch --no-cache --config ./test/jest-e2e.json",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:studio:dev:run": "prisma studio",
    "prisma:studio:test:run": "dotenv -e .env.test -- prisma studio",
    "client:analyze": "source-map-explorer 'build/static/js/*.js'",
    "client:build": "react-app-rewired build",
    "client:start": "react-app-rewired start",
    "client:test": "react-app-rewired test",
    "eject": "react-app-rewired eject"
  },
  "lint-staged": {
    "client/src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:e2e"
    }
  },
  "dependencies": {
    "@emotion/react": "11.10.0",
    "@emotion/styled": "11.10.0",
    "@mui/icons-material": "5.8.4",
    "@mui/material": "5.9.3",
    "@nestjs/common": "8.0.0",
    "@nestjs/config": "2.0.0",
    "@nestjs/core": "8.0.0",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/passport": "8.2.1",
    "@nestjs/platform-express": "8.0.0",
    "@nestjs/swagger": "6.0.5",
    "@prisma/client": "3.12.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "13.5.0",
    "@types/bcrypt": "5.0.0",
    "@types/cookie-parser": "1.4.3",
    "@types/jest": "29.0.3",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "bcrypt": "5.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "cookie-parser": "1.4.6",
    "joi": "17.6.0",
    "lodash": "4.17.21",
    "passport": "0.5.2",
    "passport-jwt": "4.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.2.0",
    "source-map-explorer": "2.5.2",
    "swagger-ui-express": "4.5.0",
    "uuid": "8.3.2",
    "web-vitals": "2.1.4"
  },
  "devDependencies": {
    "@babel/preset-react": "7.18.6",
    "@nestjs/cli": "8.0.0",
    "@nestjs/schematics": "8.0.0",
    "@nestjs/testing": "8.0.0",
    "@types/express": "4.17.13",
    "@types/lodash": "4.14.185",
    "@types/node": "16.11.59",
    "@types/passport-jwt": "3.0.6",
    "@types/supertest": "2.0.11",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "dotenv-cli": "5.1.0",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "jest": "27.2.5",
    "lint-staged": "12.3.7",
    "pactum": "3.1.5",
    "prettier": "2.3.2",
    "prisma": "3.15.0-dev.20",
    "react-app-rewired": "2.2.1",
    "source-map-support": "0.5.20",
    "supertest": "6.1.3",
    "ts-loader": "9.2.3",
    "ts-node": "10.0.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=16"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
